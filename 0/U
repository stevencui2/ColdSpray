FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (7 0 0);

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    #include "emptySymmetry.include"

    vert-outlet
    {
        type            zeroGradient;
    }
    horiz-outlet
    {
        type            zeroGradient;
    }
    left-walls
    {
        type            fixedValue;
        value		uniform (0 0 0);
    }
    nozzle-walls
    {
        type            fixedValue;
        value		uniform (0 0 0);
    }
    
}

